a=[[12.1,34.5],[22.7,10.9]]
b=[[3,4],[2,1]]

m1=(a[0][0]+a[1][1])*(b[0][0]+b[1][1])
m2=(a[1][0]+a[1][1])*b[0][0]
m3=(b[0][1]-b[1][1])*a[0][0]
m4=(b[1][0]-b[0][0])*a[1][1]
m5=(a[0][0]+a[0][1])*b[1][1]
m6=(a[1][0]-a[0][0])*(b[0][0]+b[0][1])
m7=(a[0][1]-a[1][1])*(b[1][0]+b[1][1])
c=[[0,0],[0,0]]
c[0][0]=m1+m4-m5+m7
c[0][1]=m3+m5
c[1][0]=m2+m4
c[1][1]=m1+m3-m2+m6

for i in range(2):
    for j in range(2):
        print(c[i][j],end=" ")
    print()
        
